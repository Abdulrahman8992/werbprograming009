<body>
    <form action="" method="POST">
        <label for="">Name</label>
        <input type="text" name="username"><br>
        <lablel for="">Pass</lablel>
        <input type="password"name="password"><br>
        <button name="submit"type="submit">submit</button><br>

    </form>
    <table border="2">
        <thead>
            <th>ID</th>
            <th>NAME</th>
        </thead>
 
</body>
</html>
<?php
$con=mysqli_connect("localhost","root","","abdul");


if(isset($_POST['submit'])){
    $fname=$_POST['username'];
    $pass=$_POST['password'];
    $insert="INSERT INTO user(name,password) VALUES('$fname','$pass')";
    if(mysqli_query($con,$insert))
    {echo "<script>alert('new data');
        
        </script>";}
}
$select=mysqli_query($con,"SELECT * FROM user");
while($row=mysqli_fetch_assoc($select)){
    ?>
    <tbody>
        <td><?=$row['id']?></td>
        <td><?=$row['name']?></td>
    </tbody>
    <?php
}
?>








