    <script>
        function number (){
        var a = prompt("A dakhil bka :")
        var b = prompt("B dakhil bka :")
        var c = prompt("C dakhil bka :")
        if (a==b &&  a==c && c==b)
        {
            document.write("wak yakn");

        }
        if(a==b && a!=c)
        {
            document.write("a w b wak yakn");
        }
        if(a!=b && a==c){
            document.write("a w c wak yakn")
        }
        else{
            document.write("hichyan wak yaknen")
        }
       
        
    
        }
    </script>
<script>
    function date(){
        document.write(Date());
    }
</script>
<script>
function array(){
    var x = ["orange","grapes","fig"]
    document.write(x+"<br>");
    x[3]="mango";
    document.write(x);
}
</script>
<script>
    function hour(){
        var x = new Date();
        d=x.getHours();
        document.write(d)
    }
</script>
<script>
    function weekday(){
        var x = new Date();
        d=x.getDay();
        document.write(d)
        switch(d){

            case 0:
            document.write("today is sunday");
            break;
            case 1:
            document.write("today is monday");
            break;
            case 2:
            document.write("today is tuseday");
            break;
            case 3:
            document.write("today is wendsday");
            break;
            case 4:
            document.write("today is turseday");
            break;
            case 5:
            document.write("today is friday");
            break;
            case 6:
            document.write("today is saturday");
            break;



        }
    }
</script>
<script>
    function mouseup(){
        document.write("the propose finishid ")
    }
</script>

